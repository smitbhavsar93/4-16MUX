module dec4to16_vlg_vec_tst();
// constants                                           
// general purpose registers
reg En;
reg [3:0] w;
// wires                                               
wire [15:0] y;

wire sampler;                             

// assign statements (if any)                          
dec4to16 i1 (
// port map - connection between master ports and signals/registers   
	.En(En),
	.w(w),
	.y(y)
);

// En
initial
begin
	En = 1'b1;
end 
// w[ 3 ]
always
begin
	w[3] = 1'b0;
	w[3] = #80000 1'b1;
	#80000;
end 
// w[ 2 ]
always
begin
	w[2] = 1'b0;
	w[2] = #40000 1'b1;
	#40000;
end 
// w[ 1 ]
always
begin
	w[1] = 1'b0;
	w[1] = #20000 1'b1;
	#20000;
end 
// w[ 0 ]
always
begin
	w[0] = 1'b0;
	w[0] = #10000 1'b1;
	#10000;
end 

dec4to16_vlg_sample_tst tb_sample (
	.En(En),
	.w(w),
	.sampler_tx(sampler)
);

dec4to16_vlg_check_tst tb_out(
	.y(y),
	.sampler_rx(sampler)
);
endmodule

