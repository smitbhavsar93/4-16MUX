`timescale 1 ps/ 1 ps
module dec4to16_vlg_sample_tst(
	En,
	w,
	sampler_tx
);
input  En;
input [3:0] w;
output sampler_tx;

reg sample;
time current_time;
always @(En or w)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

